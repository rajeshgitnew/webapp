def MyName = "Rajesh"

pipeline {
agent any 
stages {
stage ("print variable") {
steps { 
echo "My Name is ${MyName}"
    }
   }
stage ("compile") {
steps {
//git url: 'https://github.com/rajeshgitnew/java.git', branch: 'master'
sh "mvn -DskipTests clean  install"
   }
 }
  stage('Docker Build') {
      agent any
      steps {
        sh 'sudo docker build -t javasamplecode:latest -f /var/lib/jenkins/workspace/Jenkins_Pipeline@2/dockerfile .'
        //sh "sudo docker stop javasamplcode"
        //sh " sudo docker rm javasamplcode"
        sh "sudo docker run -p 5000:5000 -d javasamplecode --name javasamplecode"
        }
        }
    }
 }   
